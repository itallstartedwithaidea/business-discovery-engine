{
  "name": "business-discovery-engine",
  "version": "5.0.0",
  "description": "Apollo-level business intelligence from 100% public data. 4 discovery sources, social media extraction, business age scoring, live dashboard.",
  "main": "engine.js",
  "type": "module",
  "scripts": {
    "start": "node engine.js start --state AZ",
    "start:az": "node engine.js start --state AZ --fresh",
    "start:nv": "node engine.js start --state NV --fresh",
    "start:oh": "node engine.js start --state OH --fresh",
    "start:id": "node engine.js start --state ID --fresh",
    "start:wa": "node engine.js start --state WA --fresh",
    "all": "node engine.js start --state AZ --fresh && node engine.js start --state NV --fresh && node engine.js start --state OH --fresh && node engine.js start --state ID --fresh && node engine.js start --state WA --fresh",
    "test": "node engine.js start --state AZ --max 10 --categories plumber --no-sheets",
    "pause": "node engine.js pause",
    "resume": "node engine.js resume",
    "stop": "node engine.js stop",
    "status": "node engine.js status",
    "reset": "node engine.js reset",
    "help": "node engine.js help"
  },
  "dependencies": {
    "axios": "^1.7.0",
    "cheerio": "^1.0.0",
    "dotenv": "^16.4.0",
    "googleapis": "^140.0.0",
    "puppeteer": "^24.0.0",
    "puppeteer-extra": "^3.3.0",
    "puppeteer-extra-plugin-stealth": "^2.11.0"
  },
  "optionalDependencies": {
    "https-proxy-agent": "^7.0.0"
  },
  "engines": { "node": ">=18.0.0" },
  "author": "John Williams <john@itallstartedwithaidea.com>",
  "license": "MIT"
}
